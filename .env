# STF Docker Environment Configuration
# Replace localhost with your machine's IP if accessing from other devices
STF_PUBLIC_IP=localhost

# STF Admin Configuration
STF_ADMIN_EMAIL=administrator@fakedomain.com
STF_ADMIN_NAME=Administrator

# Timezone (optional, default: UTC)
TZ=UTC

# RethinkDB Configuration
RETHINKDB_HOST=rethinkdb
RETHINKDB_PORT=28015

# STF Ports
STF_WEB_PORT=7100
STF_WEBSOCKET_PORT=7110
STF_PROVIDER_MIN_PORT=7400
STF_PROVIDER_MAX_PORT=7500

# ADB Configuration
ADB_PORT=5037
